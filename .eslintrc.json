{
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "plugins": ["node"],
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "node/no-unsupported-features": "off"
  }
}
